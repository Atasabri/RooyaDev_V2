@model IEnumerable<Banner>
@using RooyaDev.Models
@{
    ViewBag.Title =SiteResource.RooayaDevelopment ;
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    string code = "en";
    string aa = "right";
    if (Request.Cookies["Language"] != null)
    {
        if (Request.Cookies["Language"].Value == "ar")
        {
            code = "ar";
            aa = "left";
        }
    }
}

<div class="Main-Slider mainslider">
    <!--
    #################################
            - THEMEPUNCH BANNER -
    #################################
    -->
    <div class="tp-banner-container">
        <div class="tp-banner">
            <ul>
                @if (code == "en")
                {
                    foreach (var item in Model)
                    {
                        <li data-transition="random" data-slotamount="7" data-masterspeed="1000" data-saveperformance="off" data-title="Slide" style="background-image: url(../../Uploads/Banner/@(item.ID).jpg); padding-top: 0; ">
                            <div class="container">
                                <div class="tp-caption light_heavy_70_shadowed">
                                    <ul class="text">
                                        <li class="tp-caption sft"
                                            data-speed="1500"
                                            data-start="1500"
                                            data-easing="Power4.easeOut"
                                            data-endspeed="500"
                                            data-endeasing="Power1.easeIn"
                                            data-captionhidden="off"><div class='BigTitle'>@item.FirstHead</div></li>
                                            <li class="tp-caption sfb"
                                                data-speed="1500"
                                                data-start="2500"
                                                data-easing="Power4.easeOut"
                                                data-endspeed="500"
                                                data-endeasing="Power1.easeIn"
                                                data-captionhidden="off">@item.SecondHead</li>
                                            </ul>
                                            <ul class="buttonss">
                                                <li class="buttons tp-caption sfr"
                                                    data-speed="1500"
                                                    data-start="3500"
                                                    data-easing="Power4.easeOut"
                                                    data-endspeed="500"
                                                    data-endeasing="Power1.easeIn"
                                                    data-captionhidden="off">
                                                    <a href="@item.Link">@SiteResource.ReadMore</a>
                                                </li>
                                                <li class="buttons tp-caption sfl"
                                                    data-speed="1500"
                                                    data-start="4500"
                                                    data-easing="Power4.easeOut"
                                                    data-endspeed="500"
                                                    data-endeasing="Power1.easeIn"
                                                    data-captionhidden="off">
                                                    <a href="~/Home/Pricing">@SiteResource.Order</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>

                    }
                }
                else
                {
                    foreach (var item in Model)
                    {
                        <li data-transition="random" data-slotamount="7" data-masterspeed="1000" data-saveperformance="off" data-title="Slide" style="background-image: url(../../Uploads/Banner/@(item.ID).jpg); padding-top: 0; ">
                            <div class="container">
                                <div class="tp-caption light_heavy_70_shadowed">
                                    <ul class="text">
                                        <li class="tp-caption sft"
                                            data-speed="1500"
                                            data-start="1500"
                                            data-easing="Power4.easeOut"
                                            data-endspeed="500"
                                            data-endeasing="Power1.easeIn"
                                            data-captionhidden="off"><div class='BigTitle'>@item.FirstHead_AR</div></li>
                                            <li class="tp-caption sfb"
                                                data-speed="1500"
                                                data-start="2500"
                                                data-easing="Power4.easeOut"
                                                data-endspeed="500"
                                                data-endeasing="Power1.easeIn"
                                                data-captionhidden="off">@item.SecondHead_AR</li>
                                            </ul>
                                            <ul class="buttonss">
                                                <li class="buttons tp-caption sfr"
                                                    data-speed="1500"
                                                    data-start="3500"
                                                    data-easing="Power4.easeOut"
                                                    data-endspeed="500"
                                                    data-endeasing="Power1.easeIn"
                                                    data-captionhidden="off">
                                                    <a href="@item.Link">@SiteResource.ReadMore</a>
                                                </li>
                                                <li class="buttons tp-caption sfl"
                                                    data-speed="1500"
                                                    data-start="4500"
                                                    data-easing="Power4.easeOut"
                                                    data-endspeed="500"
                                                    data-endeasing="Power1.easeIn"
                                                    data-captionhidden="off">
                                                    <a href="~/Home/Pricing">@SiteResource.Order</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>

                    }
                }


            </ul>
            <div class="tp-bannertimer"></div>
        </div>
    </div>
    <div class="scroll-down" style="background-image:url(../../images/scroll-down.png)"></div>
</div>

