        </div>
        <p class="flipInX animated wow">@SiteResource.EnterYourEmail</p>
        <form id="CheckDomain" action="~/Home/Subscribe" method="post">
            <div class="domsec">
                <input type="email"s required name="Email" placeholder="@SiteResource.EnterYourEmail">
